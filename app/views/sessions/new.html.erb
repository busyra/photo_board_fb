 <div class="col-md-3 col-md-offset-5">


	<h4>Log In</h4>


	<%= simple_form_for :login do |f| %>
		<div class = "form-group">
			<%= f.input :email, {class: 'form-control'} %>
		</div>

		<div class = "form-group">
			<%= f.label :password %>
			<%= f.password_field :password, {class: 'form-control'}%>
		</div>
		<div>
			<%= f.submit 'Log In' %>
		</div>
	<% end %>
</div>